    "gsap": "^3.10.4",
    "phy-touch": "^0.1.1",
    "pinia": "^2.0.12",
    "pixi.js": "^5.3.11",
    "register-service-worker": "^1.7.2",
    "stats.js": "^0.17.0",
    "three": "^0.140.2",
    "vconsole": "^3.14.3",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
